<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <display-name>Stock Search Web Application</display-name>

    <servlet>
        <servlet-name>StockSearch</servlet-name>
        <servlet-class>servlets.StockSearchServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>StockSearch</servlet-name>
        <url-pattern>/servlets/StockSearch</url-pattern>
    </servlet-mapping>

</web-app>
